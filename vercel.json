{
    "version": 2,
    "buildCommand": "node custom-build.js",
    "outputDirectory": "dist",
    "headers": [
        {
            "source": "/assets/(.*\\.glb)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "model/gltf-binary"
                }
            ]
        }
    ]
}